{
  "configurations": [
    {
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SuppliersApi__Assemblies__0": "nhs.notify.suppliers.api.letter",
        "SuppliersApi__Assemblies__1": "nhs.notify.suppliers.api.data",
        "SuppliersApi__Data": "true",
        "SuppliersApi__Letters": "true"
      },
      "name": "Server Host",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net8.0/nhs.notify.suppliers.api.host.dll",
      "request": "launch",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "stopAtEntry": false,
      "type": "coreclr"
    },
    {
      "name": ".NET Core Attach",
      "request": "attach",
      "type": "coreclr"
    }
  ],
  "version": "0.2.0"
}
